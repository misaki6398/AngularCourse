{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mod01": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/mod01",
      "sourceRoot": "projects/mod01/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mod01",
            "index": "projects/mod01/src/index.html",
            "main": "projects/mod01/src/main.ts",
            "polyfills": "projects/mod01/src/polyfills.ts",
            "tsConfig": "projects/mod01/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/mod01/src/favicon.ico",
              "projects/mod01/src/assets"
            ],
            "styles": [
              "projects/mod01/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/mod01/src/environments/environment.ts",
                  "with": "projects/mod01/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mod01:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mod01:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mod01:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mod01/src/test.ts",
            "polyfills": "projects/mod01/src/polyfills.ts",
            "tsConfig": "projects/mod01/tsconfig.spec.json",
            "karmaConfig": "projects/mod01/karma.conf.js",
            "assets": [
              "projects/mod01/src/favicon.ico",
              "projects/mod01/src/assets"
            ],
            "styles": [
              "projects/mod01/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/mod01/tsconfig.app.json",
              "projects/mod01/tsconfig.spec.json",
              "projects/mod01/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/mod01/e2e/protractor.conf.js",
            "devServerTarget": "mod01:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mod01:serve:production"
            }
          }
        }
      }
    },
    "mod02": {
      "projectType": "application",
